{
  "name": "mymusiclist",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "author": "Wilson Oh <oh.wilson123@gmail.com>",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint_d \"src/**/*.{js,jsx}\" --quiet",
    "fmt": "prettier --write \"src/**/*.{js,jsx}\"",
    "fmt:check": "prettier --check \"src/**/*.{js,jsx}\"",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx,tsx}": [
      "eslint --quiet --cache --fix"
    ],
    "*.{json,js,ts,jsx,tsx,html}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@fsouza/prettierd": "^0.22.5",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint_d": "^12.2.1",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "react-select": "^5.7.0",
    "vite": "^4.0.0"
  },
  "browserslist": [
    "last 2 Firefox versions",
    "last 2 Chrome versions"
  ]
}
